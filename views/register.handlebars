<form method="POST" action="/api/users/signUp" enctype="application/json" class="d-flex justify-content-around" id="loginForm">
    <div class="border border-primary rounded p-4 m-3">
        <h2 class="text-center m-2">Register</h3>
        <div class="form-group m-2">
            <label id="userLabel">Username</label>
            <input type="text" name="username" class="form-control">
        </div>
        <div class="form-group m-2">
            <label id="passLabel">Password</label>
            <input type="password" name="password" class="form-control">
        </div>
        <div class="text-center"> 
            <input type="submit" value="Submit"/>
        </div>
        <br>
        <div>
            <label>Already have an account? <a href="/login">Login Here</a></label>
        </div>
    </div>
</form>